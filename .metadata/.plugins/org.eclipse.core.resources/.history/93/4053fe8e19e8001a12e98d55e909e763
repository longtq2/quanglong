package com.vn.controler;

public class Calculator {
	private int a;
	private int b;
	private String c;
	private int calculator() {
		// TODO Auto-generated method stub
		switch (c) {
		case "+":
			return a + b;
		case "-":
			return a - b;
		case "*":
			return a * b;
		default:
			return a / b;
		}
	}
}
