package com.vn;

public class Main {
	public static void main(String[] args) {
		System.out.println("hello");
		int a = 0;
		int b = 0;
		String c = "+";
		calculator(a, b, c);
  
	}

	private static void calculator(int a, int b, String c) {
		// TODO Auto-generated method stub
		switch (c) {
		case "+":
			c = a + b;
			break;

		default:
			break;
		}
	}
}
