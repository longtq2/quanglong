package com.vn;

public class Main {
	public static void main(String[] args) {
		System.out.println("hello");
		int a = 5;
		int b = 0;
		String c = "+";
		Main main = new Main();
		int result = main.calculator(a, b, c);
		System.out.println(result);
  
	}

	private int calculator(int a, int b, String c) {
		// TODO Auto-generated method stub
		switch (c) {
		case "+":
			return a + b;
		case "-":
			return a - b;
		case "*":
			return a * b;
		default:
			return a / b;
		}
	}
}
